{
  "name": "io-services-cms-backoffice",
  "version": "1.10.10",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "yarn generate && next build",
    "build:pre-package": "cp -r .next/static .next/standalone/apps/backoffice/.next && cp -r public .next/standalone/apps/backoffice",
    "build:package": "yarn build:pre-package && (cd .next/standalone/apps/backoffice && zip -r -q ../../../../${npm_package_name}-${npm_package_version}.zip . -x package.json)",
    "start": "./node_modules/next/dist/bin/next start",
    "lint": "next lint",
    "test": "yarn test:b4f && yarn test:fe",
    "test:b4f": "vitest run --config ./vitest.config.node.ts",
    "test:fe": "vitest run --config ./vitest.config.react.ts",
    "coverage": "yarn coverage:b4f && yarn coverage:fe",
    "coverage:b4f": "vitest run --config ./vitest.config.node.ts --coverage",
    "coverage:fe": "vitest run --config ./vitest.config.react.ts --coverage",
    "api-definitions:clean": "rm -rf ./src/generated/api",
    "api-definitions:generate": "gen-api-models --api-spec ./openapi.yaml --out-dir ./src/generated/api --request-types --response-decoders --client",
    "api-definitions:all": "yarn api-definitions:clean && yarn api-definitions:generate",
    "services-cms:clean": "rm -rf ./src/generated/services-cms",
    "services-cms:generate": "gen-api-models --api-spec ../io-services-cms-webapp/openapi.yaml --out-dir ./src/generated/services-cms --request-types --response-decoders --client",
    "services-cms:all": "yarn services-cms:clean && yarn services-cms:generate",
    "selfcare-api-models:clean": "rm -rf ./src/types/selfcare",
    "selfcare-api-models:generate": "gen-api-models --api-spec ./selfcare-openapi.yaml --out-dir ./src/types/selfcare --request-types --response-decoders",
    "selfcare-api-models": "yarn selfcare-api-models:clean && yarn selfcare-api-models:generate",
    "generate": "yarn api-definitions:all && yarn services-cms:all"
  },
  "dependencies": {
    "@azure/arm-apimanagement": "8.1.2",
    "@azure/cosmos": "^3.17.3",
    "@azure/identity": "^3.1.4",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.1",
    "@io-services-cms/external-clients": "workspace:^",
    "@io-services-cms/models": "workspace:^",
    "@mui/icons-material": "^5.14.3",
    "@mui/lab": "^5.0.0-alpha.142",
    "@mui/material": "^5.14.5",
    "@mui/system": "^5.14.5",
    "@opentelemetry/instrumentation-undici": "^0.3.0",
    "@pagopa/io-functions-commons": "^28.2.0",
    "@pagopa/mui-italia": "^1.0.1",
    "applicationinsights": "^3.1.0",
    "axios": "^1.6.5",
    "eslint": "8.50.0",
    "eslint-config-next": "^13.5.6",
    "fp-ts": "^2.13.1",
    "i18next": "^23.5.1",
    "io-ts": "^2.2.20",
    "jose": "^4.15.5",
    "lodash": "^4.17.21",
    "next": "^13.5.6",
    "next-auth": "^4.24.5",
    "next-i18next": "^14.0.3",
    "notistack": "^3.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.47.0",
    "react-i18next": "^13.2.2",
    "react-markdown": "^9.0.0",
    "xss": "^1.0.14",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@pagopa/openapi-codegen-ts": "^13.1.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/lodash": "^4.14.202",
    "@types/node": "20.4.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.0.4",
    "jsdom": "^23.0.1",
    "msw": "^2.0.11",
    "typescript": "5.1.6",
    "url-loader": "^4.1.1",
    "vite": "^5.0.12",
    "vitest": "^1.0.4"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
