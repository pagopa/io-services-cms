{
  "name": "io-services-cms-webapp",
  "version": "1.26.7",
  "packageManager": "yarn@3.3.0",
  "files": [
    "**/function.json",
    "dist",
    "host.json",
    "extensions.csproj"
  ],
  "bundledDependencies": true,
  "scripts": {
    "build": "rm -rf ./dist & tsc",
    "build:package": "npm-pack-zip --add-version",
    "format": "prettier --write .",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**\"",
    "lint:fix": "eslint --fix \"src/**\"",
    "start": "dotenv -e .env func start -- --javascript",
    "generate": "rm -rf ./src/generated/api & gen-api-models --api-spec ./openapi.yaml --out-dir ./src/generated/api --request-types --response-decoders"
  },
  "devDependencies": {
    "@pagopa/eslint-config": "^3.0.0",
    "@pagopa/openapi-codegen-ts": "^13.1.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/express": "^4.17.17",
    "@types/pg": "^8.6.6",
    "@types/pg-cursor": "^2.7.0",
    "@types/supertest": "^2.0.12",
    "@types/upng-js": "^2.1.1",
    "@vitest/coverage-v8": "^1.0.4",
    "azure-functions-core-tools": "4",
    "eslint": "^8.38.0",
    "npm-pack-zip": "^1.3.0",
    "prettier": "^2.8.7",
    "supertest": "^6.3.3",
    "typescript": "^5.0.4",
    "vitest": "^1.0.4"
  },
  "dependencies": {
    "@azure/arm-apimanagement": "8.1.2",
    "@azure/cosmos": "^3.17.3",
    "@azure/functions": "^3.5.1",
    "@azure/identity": "^3.1.4",
    "@io-services-cms/external-clients": "workspace:^",
    "@io-services-cms/models": "workspace:^",
    "@pagopa/express-azure-functions": "^4.0.1",
    "@pagopa/io-functions-commons": "^28.2.0",
    "@pagopa/ts-commons": "^11.0.0",
    "body-parser": "^1.20.2",
    "dotenv-cli": "^7.2.1",
    "express": "^4.18.2",
    "fp-ts": "^2.13.1",
    "io-ts": "^2.2.20",
    "io-ts-types": "^0.5.19",
    "knex": "^2.4.2",
    "monocle-ts": "^2.3.13",
    "newtype-ts": "^0.3.5",
    "node-fetch-commonjs": "^3.2.4",
    "pg": "^8.10.0",
    "pg-cursor": "^2.9.0",
    "upng-js": "^2.1.0"
  },
  "imports": {
    "#package.json": "./package.json"
  }
}
